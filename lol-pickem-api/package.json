{
  "name": "lol-pickem-api",
  "version": "1.0.0",
  "description": "The API for the LolPickem App",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --build",
    "build:watch": "tsc --watch",
    "clean": "clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "start:watch": "tsc-watch --onSuccess \"node -r source-map-support/register .\"",
    "prepublishOnly": "npm run test"
  },
  "author": "Sydney Heller",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/compression": "1.7.0",
    "@types/express": "^4.17.3",
    "@types/mongoose": "^5.7.5",
    "@types/node": "^11.9.4",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^7.0.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jwks-rsa": "^1.7.0",
    "mongoose": "^5.9.4",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "concurrently": "^5.1.0",
    "eslint": "^6.8.0",
    "source-map-support": "^0.5.16",
    "tsc-watch": "^4.2.3"
  }
}
